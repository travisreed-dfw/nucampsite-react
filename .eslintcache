nt {\n    constructor(props) {\n    super(props);\n    this.state = {\n        selectedCampsite: null,\n    };\n    }\n\n    renderCampsite(campsite) {\n        if (campsite) {\n            return (\n                <div className=\"col-md-5 m-1\">\n                    <Card>\n                        <CardImg top src={campsite.image} alt={campsite.name} />\n                        <CardBody>\n                        <CardTitle>{campsite.name}</CardTitle>\n                        <CardText>{campsite.description}</CardText>\n                        </CardBody>\n                    </Card>\n                </div>\n            );\n        }\n        return <div />;\n    }\n\n    renderComments(comments) {\n        if (comments) {\n            return (\n                <div className=\"col-md-5 m-1\">\n                    <h4>Comments</h4>\n                    {comments.map(comment => \n                    <div key={comment.id}> \n                    {comment.text} <br />\n                    -- {comment.author}, {new Intl.DateTimeFormat('en-US', { year: 'numeric', month: 'short', day: '2-digit'}).format(new Date(Date.parse(comment.date)))}\n                    </div>)}\n                </div>\n            );\n        }\n        return <div />;\n    }\n\n    render() {\n        if (this.props.campsite) {\n            return (<div className=\"row\">\n                {this.renderCampsite(this.props.campsite)} \n                {this.renderComments(this.props.campsite.comments)}\n            </div>);\n        } else {\n            return (<div></div>);\n        }\n    }\n}\n\nexport default CampsiteInfo;\n",{"ruleId":"35","severity":1,"message":"36","line":5,"column":8,"nodeType":"37","messageId":"38","endLine":5,"endColumn":20},{"ruleId":"39","replacedBy":"40"},{"ruleId":"41","replacedBy":"42"},{"ruleId":"35","severity":1,"message":"43","line":2,"column":41,"nodeType":"37","messageId":"38","endLine":2,"endColumn":49},{"ruleId":"35","severity":1,"message":"44","line":2,"col